<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="suhyeon" class="com.fw.week2.Person" depends-on="financier">
    <property name="name" value="suhyeon"/>
    <property name="age" value="26"/>
    <property name="gender" value="F"/>
    <property name="birthDate" value="2001-09-20"/>
    <property name="food" ref="financier"/>
  </bean>

  <bean id="financier" class="com.fw.week3.Food">
    <property name="name" value="financier"/>
    <property name="type" value="dessert"/>
  </bean>

</beans>